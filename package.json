{
  "name": "cjdquick-logistics",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "db:studio": "pnpm --filter @cjdquick/database db:studio",
    "clean": "turbo clean && rm -rf node_modules",
    "oms:install": "cd oms && npm install",
    "oms:dev": "cd oms && npm run dev",
    "oms:build": "cd oms && npm run build",
    "oms:db:push": "cd oms/packages/database && npx prisma db push",
    "oms:db:seed": "cd oms/packages/database && npx prisma db seed"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=20"
  }
}
